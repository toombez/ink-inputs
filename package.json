{
  "name": "ink-inputs",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "packages/*",
    "apps/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev --parallel",
    "examples": "cd apps/examples && yarn run dev",

    "libs:dev": "yarn run dev --filter=\"!./apps/*\"",
    "libs:build": "yarn run build --filter=\"!@ink-inputs\\apps\""
  },
  "engines": {
    "node": ">=16"
  },
  "devDependencies": {
    "turbo": "^1.9.4"
  }
}
